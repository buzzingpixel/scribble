{
    "name": "buzzingpixel/scribble",
    "description": "Markdown file content",
    "version": "1.0.0",
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.26.2",
        "doctrine/coding-standard": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpmd/phpmd": "^2.6",
        "phpunit/phpunit": "^8.1",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "BuzzingPixel\\Scribble\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BuzzingPixel\\Tests\\": "tests/"
        }
    },
    "config": {
        "bin-compat": "full",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "diConfigFilePath": "config/di.php"
    }
}

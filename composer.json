{
    "name": "buzzingpixel/scribble",
    "description": "Markdown file content",
    "version": "1.0.0",
    "license": "Apache-2.0",
    "require": {
        "php": ">=7.2",
        "adbario/php-dot-notation": "^2.2",
        "buzzingpixel/corbomite-di": "^1.2",
        "cebe/markdown": "^1.2",
        "hyn/frontmatter": "dev-master",
        "symfony/finder": "^4.2"
    },
    "require-dev": {
        "buzzingpixel/corbomite-cli": "^1.0",
        "codedungeon/phpunit-result-printer": "^0.26.2",
        "doctrine/coding-standard": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpmd/phpmd": "^2.6",
        "phpstan/phpstan": "^0.11.5",
        "phpunit/phpunit": "^8.1",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4",
        "symfony/var-dumper": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "BuzzingPixel\\Scribble\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BuzzingPixel\\Tests\\": "tests/",
            "BuzzingPixel\\Work\\": "work/"
        }
    },
    "config": {
        "bin-compat": "full",
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "cliActionConfigFilePath": "config/cliConfig.php",
        "diConfigFilePath": "config/di.php"
    }
}
